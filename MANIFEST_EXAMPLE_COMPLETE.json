{
  "version": "1.0.0",
  "source": "newgen-studio",
  "exportId": "exp_20251210_plugin_marketplace",
  "timestamp": "2025-12-10T12:34:56.000Z",

  "project": {
    "id": "proj_plugin_marketplace_01",
    "name": "NewGen Plugin Marketplace",
    "domain": "biologics",
    "type": "studio-app",
    "tags": ["plugins", "marketplace", "ai-builder"],
    "description": "Marketplace of scientifically validated plugins for biologics research.",
    "domainMeta": {
      "moleculeType": "protein",
      "phase": "discovery",
      "therapeuticArea": "immunology",
      "regulatoryContext": "non-GLP research"
    }
  },

  "layout": {
    "rootRoute": "plugins",
    "routes": [
      {
        "id": "route_plugins",
        "path": "/plugins",
        "title": "Plugin Marketplace",
        "layout": "app-shell-left-nav",
        "navGroup": "main",
        "icon": "plug",
        "pageComponentId": "page_plugins"
      }
    ],
    "components": [
      {
        "id": "page_plugins",
        "type": "Page",
        "props": {
          "title": "Plugin Marketplace",
          "subtitle": "Discover scientifically-validated plugins for biologics research"
        },
        "children": [
          "cmp_plugins_filters",
          "cmp_plugins_list"
        ]
      },
      {
        "id": "cmp_plugins_filters",
        "type": "HorizontalFilterBar",
        "props": {
          "searchPlaceholder": "Search plugins by name or keyword",
          "filters": [
            {
              "id": "category",
              "label": "All Categories",
              "type": "select",
              "optionsDataSourceId": "ds_plugin_categories"
            },
            {
              "id": "license",
              "label": "All Licenses",
              "type": "select",
              "options": [
                { "value": "all", "label": "All Licenses" },
                { "value": "free", "label": "Free" },
                { "value": "commercial", "label": "Commercial" }
              ]
            }
          ]
        },
        "bindings": {
          "onChange": "action_updatePluginFilters"
        }
      },
      {
        "id": "cmp_plugins_list",
        "type": "CardList",
        "props": {
          "title": "Plugins",
          "emptyState": {
            "title": "No plugins found",
            "body": "Try adjusting your search or filters."
          }
        },
        "bindings": {
          "items": "ds_plugins.data.items"
        },
        "itemTemplate": "cmp_plugin_card"
      },
      {
        "id": "cmp_plugin_card",
        "type": "Card",
        "slots": {
          "header": {
            "type": "Text",
            "bindings": {
              "text": "item.name"
            }
          },
          "tagline": {
            "type": "Text",
            "bindings": {
              "text": "item.tagline"
            }
          },
          "meta": {
            "type": "InlineMeta",
            "bindings": {
              "left": "item.provider",
              "right": "item.category"
            }
          },
          "badges": {
            "type": "BadgeRow",
            "bindings": {
              "items": "item.badges"
            }
          },
          "footer": {
            "type": "ButtonRow",
            "props": {
              "primaryLabel": "View details"
            },
            "bindings": {
              "onPrimaryClick": "action_openPluginDetails"
            }
          }
        }
      }
    ]
  },

  "dataSources": [
    {
      "id": "ds_plugins",
      "type": "http",
      "label": "Plugin catalog",
      "config": {
        "method": "GET",
        "url": "/api/v1/plugins",
        "queryParams": {
          "q": "{{state.search}}",
          "category": "{{state.filters.category}}",
          "license": "{{state.filters.license}}"
        }
      },
      "schema": {
        "itemsPath": "data.items",
        "fields": {
          "id": "string",
          "name": "string",
          "tagline": "string",
          "provider": "string",
          "category": "string",
          "badges": "array",
          "description": "string",
          "icon": "string",
          "featured": "boolean"
        }
      },
      "refresh": {
        "onInit": true,
        "onFilterChange": true
      }
    },
    {
      "id": "ds_plugin_categories",
      "type": "static",
      "label": "Plugin categories",
      "config": {
        "items": [
          { "value": "all", "label": "All Categories" },
          { "value": "structure", "label": "Structure Prediction" },
          { "value": "simulation", "label": "Simulation" },
          { "value": "qc", "label": "QC & Analytics" },
          { "value": "sequencing", "label": "Sequencing" },
          { "value": "imaging", "label": "Imaging" }
        ]
      }
    }
  ],

  "actions": [
    {
      "id": "action_updatePluginFilters",
      "type": "state.update",
      "label": "Update plugin filters",
      "config": {
        "targetStatePath": "filters",
        "merge": true
      },
      "sideEffects": [
        {
          "type": "datasource.refresh",
          "dataSourceId": "ds_plugins"
        }
      ]
    },
    {
      "id": "action_openPluginDetails",
      "type": "navigation.openModal",
      "label": "Open plugin details modal",
      "config": {
        "routeId": "route_plugins",
        "modalComponentId": "cmp_plugin_details_modal",
        "payload": "{{item}}"
      }
    },
    {
      "id": "action_installPlugin",
      "type": "api.call",
      "label": "Install a plugin",
      "config": {
        "method": "POST",
        "url": "/api/v1/plugins/install",
        "body": "{{item}}"
      },
      "sideEffects": [
        {
          "type": "notification.show",
          "message": "Plugin installed successfully!"
        },
        {
          "type": "datasource.refresh",
          "dataSourceId": "ds_plugins"
        }
      ]
    }
  ],

  "permissions": [
    {
      "role": "viewer",
      "description": "Can browse the plugin marketplace",
      "routes": ["route_plugins"],
      "actions": []
    },
    {
      "role": "researcher",
      "description": "Can browse plugins and install them",
      "routes": ["route_plugins"],
      "actions": ["action_updatePluginFilters", "action_openPluginDetails", "action_installPlugin"]
    },
    {
      "role": "admin",
      "description": "Can manage plugins and integrations",
      "routes": ["route_plugins"],
      "actions": ["action_updatePluginFilters", "action_openPluginDetails", "action_installPlugin"]
    }
  ],

  "theme": {
    "preset": "newgen-light",
    "colors": {
      "primary": "#7C3AED",
      "primarySoft": "#F3E8FF",
      "primaryDark": "#6D28D9",
