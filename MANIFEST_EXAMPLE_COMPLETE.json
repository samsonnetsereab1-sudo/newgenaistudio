  "deployment": {
    "target": "base44",
    "env": "staging",
    "dependencies": [
      {
        "type": "api",
        "name": "newgen-backend",
        "url": "https://api.newgen.local/api",
        "description": "NewGen Studio backend (plugins API and orchestration)",
        "methods": ["GET", "POST", "PATCH"],
        "endpoints": [
          "/v1/plugins",
          "/v1/plugins/:id",
          "/v1/plugins/install"
        ]
      },
      {
        "type": "service",
        "name": "plugin-store",
        "url": "https://plugins.newgen.local",
        "description": "Central plugin distribution service"
      }
    ],
